<?xml version="1.0"?>
<registry>
  <layoutManager class="te.jsp.JSPLayoutManager" name="default" description="default list layout">
    <property name="jspPage">/te/layout/default.jsp</property>
  </layoutManager>
  <layoutManager class="te.jsp.JSPLayoutManager" name="horizontal" description="horizontal  layout">
    <property name="jspPage">/te/layout/horizontal.jsp</property>
  </layoutManager>
  <layoutManager class="te.jsp.JSPLayoutManager" name="vertical" description="default list layout">
    <property name="jspPage">/te/layout/vertical.jsp</property>
  </layoutManager>
  <container class="te.jsp.JSPContainer" path="/te/container/default.jsp" name="horizontal" description="a container with a horizontal layout" layout="horizontal"/>
  <container class="te.jsp.JSPContainer" path="/te/container/default.jsp" name="vertical" description="a container with a vertical layout" layout="vertical"/>
  <template class="te.jsp.JSPTemplate" path="/te/template/default.jsp" name="default" description="default tempate using the default layout" layout="default"/>
  <component class="nl.vpro.ProgrammaSiteNavigationComponent" name="navigation" description="The VPRO programma sites navigation"/>
  <component class="te.jsp.JSPComponent" path="/te/component/page_head.jsp" name="page_head" description="The top of the VPRO page with meta navigation"/>
  <component class="te.jsp.JSPComponent" path="/te/component/maps.jsp" name="maps" description="Display information about maps"/>
  <component class="te.jsp.JSPComponent" path="/te/component/related_news.jsp" name="related_news" description="Display information about related news"/>
</registry>
